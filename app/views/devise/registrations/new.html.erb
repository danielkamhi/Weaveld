<div id="SignBody">
<h1 id="welcome">Welcome to Weaveld.</h1>
<h2 id="signInTitle">Sign up</h2>

	<div class="signInForm">
	    <div id="authform2" >
			<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
			  <%= devise_error_messages! %>
				<div class="field">
			  <div><%= f.label :email %><br />
			  <%= f.email_field :email, class: 'form-control', autofocus: true %></div>
				</div>
				<div class="field">
			  <div><%= f.label :password %><br />
			    <%= f.password_field :password, class: 'form-control', autocomplete: "off" %></div>
				</div>
				<div class="field">
			  <div><%= f.label :password_confirmation %><br />
			    <%= f.password_field :password_confirmation, class: 'form-control', autocomplete: "off" %></div>
				</div>
			  <div><%= f.submit "Sign up" %></div>
			<% end %>

			
		</div>
	</div>
</div>







	


